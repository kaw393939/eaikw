{
  "name": "jobclub-eleventy-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx tailwindcss -i src/css/tailwind.css -o _site/css/main.css && eleventy",
    "css": "npx tailwindcss -i src/css/tailwind.css -o _site/css/main.css --watch",
    "server": "eleventy --serve",
    "dev": "npm run build && concurrently \"npm run css\" \"npm run server\""
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.1"
  }
}
